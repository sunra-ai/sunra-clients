{
  "name": "@sunra/client",
  "description": "The sunra client for JavaScript and TypeScript",
  "version": "0.2.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sunra/sunra-js.git",
    "directory": "libs/client"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "tsc -p ./tsconfig.lib.json",
    "dev": "tsc -p ./tsconfig.lib.json --watch",
    "lint": "eslint --ext .ts src",
    "lint:fix": "npm run lint -- --fix",
    "test": "vitest run --dir ."
  },
  "keywords": [
    "sunra",
    "client",
    "ai",
    "ml",
    "typescript"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./endpoints": "./dist/types/endpoints.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@msgpack/msgpack": "^3.0.0-beta2",
    "debug": "^4.4.0",
    "eventsource-parser": "^1.1.2",
    "fetch-to-curl": "^0.6.0",
    "robot3": "^0.4.1"
  },
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "@types/debug": "^4.1.12",
    "@types/node": "^22.13.5",
    "vitest": "^3.0.7"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
