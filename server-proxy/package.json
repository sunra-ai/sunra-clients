{
  "name": "@sunra/server-proxy",
  "description": "The sunra server proxy adapter for JavaScript and TypeScript Web frameworks",
  "version": "0.1.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sunra/sunra-js.git",
    "directory": "libs/proxy"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "tsc -p ./tsconfig.lib.json",
    "dev": "tsc -p ./tsconfig.lib.json --watch",
    "lint": "eslint --ext .ts src"
  },
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "@types/debug": "^4.1.12",
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.5",
    "vitest": "^3.0.7"
  },
  "keywords": [
    "sunra",
    "client",
    "next",
    "nextjs",
    "express",
    "hono",
    "proxy"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./express": "./dist/express.js",
    "./hono": "./dist/hono.js",
    "./nextjs": "./dist/nextjs.js",
    "./remix": "./dist/remix.js",
    "./svelte": "./dist/svelte.js"
  },
  "typesVersions": {
    "*": {
      "express": [
        "src/express.d.ts"
      ],
      "hono": [
        "src/hono.d.ts"
      ],
      "nextjs": [
        "src/nextjs.d.ts"
      ],
      "remix": [
        "src/remix.d.ts"
      ],
      "svelte": [
        "src/svelte.d.ts"
      ]
    }
  },
  "main": "./src/index.js",
  "types": "./src/index.d.ts",
  "peerDependencies": {
    "@remix-run/dev": "^2.0.0",
    "@remix-run/node": "^2.0.0",
    "@sveltejs/kit": "^2.0.0",
    "express": "^4.0.0",
    "hono": "^4.0.0",
    "next": "13.4 - 14 || >=15.0.0-0",
    "react": "^18.0.0 || >=19.0.0-0",
    "react-dom": "^18.0.0 || >=19.0.0-0"
  },
  "peerDependenciesMeta": {
    "@remix-run/dev": {
      "optional": true
    },
    "@sveltejs/kit": {
      "optional": true
    },
    "express": {
      "optional": true
    },
    "hono": {
      "optional": true
    },
    "next": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "dependencies": {
    "debug": "^4.4.0"
  }
}
